@model CAYCANHNT.Models.CONTACT

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/admin/Views/Shared/_MyLayout.cshtml";
}

<div style="height:15px"></div>
<div class="panel panel-red">
    <div class="panel-heading">Xem thông tin Phản hồi</div>
    <div class="panel-body">
        <div>
            <dl class="dl-horizontal">
                <dt>
                    @Html.Raw("Thời gian gửi: ")
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.datebegin)
                </dd>

                <dt>
                    @Html.Raw("Tên khách hàng: ")
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.username)
                </dd>

                <dt>
                    @Html.Raw("Địa chỉ Email: ")
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.email)
                </dd>

                <dt>
                    @Html.Raw("Tiêu đề: ")
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.title)
                </dd>

                <dt>
                    @Html.Raw("Nội dung: ")
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.detail)
                </dd>

            </dl>
        </div>
        <p>
            @*@Html.ActionLink("Sửa", "Edit", new { id = Model.ID_CAT }, new { @class = "btn btn-success" }) |*@
            @Html.ActionLink("Quay về", "Index", "Index", new { @class = "btn btn-danger" })
        </p>
    </div>
</div>
